SAIL_VERSION_MAJOR=9
SAIL_VERSION_MINOR=0
SAIL_VERSION_PATCH=1
SAIL_VERSION="$SAIL_VERSION_MAJOR.$SAIL_VERSION_MINOR.$SAIL_VERSION_PATCH"

SAIL_VERSION_UNIQUE_NAME=`printf "_%d%03d" $SAIL_VERSION_MAJOR $SAIL_VERSION_MINOR`

SAIL_RELEASE_DATE=`date "+%B %e, %Y"`
SAIL_VERSION_STRING="Wooden Ships & Iron Men Organization: Sail $SAIL_VERSION: ($SAIL_RELEASE_DATE)"
